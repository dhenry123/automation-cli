{
  "name": "automation-cli",
  "version": "20.0.39",
  "license": "AGPL-3.0",
  "bin": "dist/automation-cli.js",
  "type": "commonjs",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "lint": "eslint -c ./eslint.config-prod.mjs 'src/**/*.ts'",
    "build": "npm run lint && npm run clean && tsc -p tsconfig.prod.json",
    "buildExecutable": "npm run build && pkg --compress GZip -o automation-cli -c ./package.json dist/automation-cli.js",
    "dev": "tsc --watch"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/adm-zip": "^0.5.7",
    "@types/express": "^5.0.0",
    "@types/humanize-duration": "^3.27.4",
    "@types/ip": "^1.1.3",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.5",
    "@types/prompts": "^2.4.9",
    "@types/ssh2": "^1.15.1",
    "@yao-pkg/pkg": "^6.2.0",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "cli-table3": "^0.6.5",
    "colors": "^1.4.0",
    "commander": "^12.1.0",
    "express": "^4.21.2",
    "humanize-duration": "^3.32.1",
    "ip": "^2.0.1",
    "minisearch": "^7.1.1",
    "morgan": "^1.10.0",
    "prompts": "^2.4.2",
    "ssh2": "^1.16.0",
    "yaml": "^2.7.0"
  },
  "pkg": {
    "scripts": "automation-cli.js",
    "assets": [
      "assets/*"
    ],
    "outputPath": ".",
    "targets": [
      "node20-linux-arm64",
      "node20-linux-x64"
    ]
  }
}
